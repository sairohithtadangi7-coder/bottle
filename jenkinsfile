pipeline{
    agent any

    stages{
        stage('checkout code'){
            steps{
                git url:https://github.com/sairohithtadangi7-coder/bottle.git
            }
        }
        stage('build'){
            steps{
                sh 'mvn clean package'
            }
        }  
        stage('test'){
            steps{
                sh 'mvn test'
            }
        }
    }
}
