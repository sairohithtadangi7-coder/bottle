pipeline{
    agent any

    stages{
        stage('git url') {
            steps{
                git url: 'https://github.com/sairohithtadangi7-coder/bottle.git'
            }
        }

        stage('cleanwk'){
            steps{
                cleanws()
            }
        }

        stage('build') {
            steps{
                sh 'mvn clean package'
            }
        }

        stage('test') {
            steps{
                sh 'mvn test'
            }
        }
    }
}