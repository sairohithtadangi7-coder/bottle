pipeline{
   agent any
   stages{
    stage('checkout') {
        steps{
            git url: https://github.com/sairohithtadangi7-coder/bottle.git

        }
    }
   }
   stages{
    stage('build') {
        steps{
            sh 'mvn clean package'
        }
    }
   }
    stages{
        stage('docker') {
            steps{
                sh 'docker build -t myapp:1 .'
            }
        }
    }
}